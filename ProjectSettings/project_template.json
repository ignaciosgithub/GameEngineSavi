            "vsync": true,
            "msaa": 4,
            "shadows": true
        }
    },
    "assetPaths": {
        "models": "Assets/Models",
        "textures": "Assets/Textures",
        "sounds": "Assets/Sounds",
        "scripts": "Assets/Scripts"
    }
}
