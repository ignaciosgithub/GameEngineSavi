{
    "projectName": "New Project",
    "engineVersion": "1.0.0",
    "buildSettings": {
        "debugSymbols": true,
        "optimization": false,
        "targetPlatforms": ["Windows", "Linux"],
        "outputDirectory": "Build"
    },
